    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "joi": "^17.13.3",
    "nanoid": "^4.0.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.11.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-markdown": "^9.0.0",
